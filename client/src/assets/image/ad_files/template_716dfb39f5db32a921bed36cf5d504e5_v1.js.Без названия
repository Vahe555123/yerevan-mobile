$(document).ready(function(){
    var $inputsPass = $('[type="password"]').parent();

    $inputsPass.append("<span class = 'password-toogle' data-toogle-pass></span>")
    
    $('body').on('click', '[data-toogle-pass]', function () {
        var x = $(this).prev();
        if (x.attr('type') === "password") {
            x.attr('type', 'text');
        } else {
            x.attr('type', 'password');
        }
    });
})



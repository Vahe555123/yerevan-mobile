/**
 * HEADER SEARCH
 */
$('.header-search__icon').click(function(e){
    e.preventDefault();
    var searchText = $('.header-search__input input').val();
    searchAction(searchText);
});

$('.header-search__input input').keydown(function(e) {
    //e.preventDefault();
    if(e.keyCode === 13) {
        var searchText = $('.header-search__input input').val();
        searchAction(searchText);
    }
});

$('.footer-search__input input').keydown(function(e) {
    //e.preventDefault();
    if(e.keyCode === 13) {
        if (!$('.header-search').hasClass('header-search_active')) {
            $('.header-search').addClass('header-search_active');
        }
        var searchText = $('.footer-search__input input').val();
        searchAction(searchText);
    }
});


function searchAction(searchText){
    if (!$('.header-search').hasClass('header-search_active')) {
        $('.header-search').addClass('header-search_active');
        $(".header-search__input input").focus();
    } else {
        var url = 'http://'+ window.location.hostname +'/catalog/';

        if (/[?&]q\s*=/.test(url)) {
            url = url.replace(/(?:([?&])q\s*=[^?&]*)/, "?q=" + searchText);
        } else if (/\?/.test(url)) {
            url = url + "&q=" + searchText;
        } else {
            url = url + "?q=" + searchText;
        }
        window.location.replace(url);
    }
}


/**
 * INDEX ASIDE LEFT
 */
function indexAsideLeft(){
    $('.aside_index').css({
        left: Math.floor((($(window).width() - $('.container').innerWidth()) / 2) + parseInt($('.container').css('padding-left'))) + 'px'
    })
}
indexAsideLeft();

/**
 * ASIDE DROPDOWN
 *
 * @param selector
 */
function asideTree(selector){
    const lis = $(selector).find('li');
    lis.each(function(e){
        if ($(this).find('ul:first').length) {
            $(this).children('a').click(function(e){
                const target = $(e.target);
                if (target.is('a') || target.is('span')) {
                    if (target.attr("href") !== '/catalog/' && !target.hasClass('noscript')) {
                        e.preventDefault();
                        if (!target.closest('li').hasClass('active')) {
                            target.closest('li').addClass('active').find('span:first').html('&nbsp;-&nbsp;');
                            target.closest('li').find('ul:first').slideDown(250);
                        } else {
                            target.closest('li').removeClass('active').find('span:first').html('&nbsp;+&nbsp;');
                            target.closest('li').find('ul:first').slideUp(250);
                        }
                    }
                }
            })
        }
    })
}
asideTree('.expandable');
asideTree('.expandable_mobile');

/**
 * WINDOW RESIZE
 */
$(window).resize(function(){
    indexAsideLeft();
    if ($(window).outerWidth() >= 1200 && $(window).height() >= 750) {
        hideBurgerNav();
    }
});


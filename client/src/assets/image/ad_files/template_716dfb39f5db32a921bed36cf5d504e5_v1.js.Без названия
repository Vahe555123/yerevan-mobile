$(document).ready(function(){

    $(document).on('change', '.section-card-cover__select select', function() {
        var murl = $("#coating_select").val();
        location.href = murl;
    });

/**
 * FIXED HEADER
  */
function fixedHeader(){
    const header = $('.header');
    let scrollPrev = 0;

    /**
     * CUSTOM SCROLLBAR
     *
     * @type {jQuery}
     */
    const instance = $('body').overlayScrollbars({
        callbacks: {
            onScroll: function(e){
                const scrolled = this.scroll().position.y;
                if (scrolled > 200) {
                    $('.scroll-to-top').fadeIn(250);
                } else {
                    $('.scroll-to-top').fadeOut(250);
                }

                if ( scrolled > 100 && scrolled > scrollPrev ) {
                    $('.header').addClass('header_out');
                } else {
                    $('.header').removeClass('header_out');
                }
                scrollPrev = scrolled;

                CheckVisibility();
                //console.log(e.target);
                //console.log(isVisible($("#kolos_8")));
            }
        }
    }).overlayScrollbars();

    // SCROLL TO TOP
    $('.scroll-to-top').click(function(){
        instance.scroll([ 0, 0 ], 1500);
    })
}
fixedHeader();

/*
*     
* */
function CheckVisibility(){

    if (!$('.aside_index').length) {
        var url=location.href;
        if (url.indexOf("/catalog/?q=")===-1 && $('.section-head__filters').length) {
            
            //$(".aside-nav").find("a.depth_level_more_two").css('color', '#000').parent().removeClass("active");
            $(".section-catalog__card h3").each(function( index ) {
                
                if(isVisible($( this ))){
                    //console.log($( this ).attr('id') );
                    $("a[href$='#"+$( this ).attr('id')+"']").css('color', '#b89d38');
                }else{
                    $("a[href$='#"+$( this ).attr('id')+"']").css('color', '#000').parent().removeClass("active");
                }
    
            });
        }

        
    }
    
}

/*
*     
* */
function isVisible($el) {
    var winTop = $(window).scrollTop();
    var winBottom = winTop + $(window).height();
    var elTop = $el.offset().top + 1;
    var elBottom = elTop + $el.height();
    return ((elBottom <= winBottom) && (elTop >= winTop));
}

/**
 *  
 */
$('.header-burger-nav').overlayScrollbars({ });
$('.aside_index').overlayScrollbars({
    callbacks: {
        onInitialized: function(e){
            $('.aside_index').css("opacity",1);
        }
    }

 });
$('.section-filters').overlayScrollbars({ });
//$('.section__aside').css("overflow","auto");
//$('.section__aside').overlayScrollbars({ });

/**
 * HEADER BURGER
 */
function showBurgerNav(){
    $('.header__burger-nav').addClass('header__burger-nav_active');
}
function hideBurgerNav(){
    $('.header__burger-nav').removeClass('header__burger-nav_active');
}
$('.header__burger').click(function(){
    showBurgerNav();
});
$('.header-burger-nav__close').click(function(){
    hideBurgerNav();
});




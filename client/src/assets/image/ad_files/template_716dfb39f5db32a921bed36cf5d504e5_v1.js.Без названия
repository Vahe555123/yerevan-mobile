 * CUSTOM SELECT
 */
$('select').niceSelect();



/**
 * FILTERS EXPAND
 */
$('.section-filters-block__expand').click(function(e){
    e.preventDefault();
    const block = $(this).closest('.section-filters-block');
    if (!block.hasClass('section-filters-block_expanded')) {
        $(this).find('a').text(' -');
        block.addClass('section-filters-block_expanded'); //.find('.section-filters-block__hidden').slideDown(250);
    } else {
        $(this).find('a').text('  +');
        block.removeClass('section-filters-block_expanded'); //.find('.section-filters-block__hidden').slideUp(250);
    }
});

/**
 * FILTERS SHOW/HIDE
 */
function showFilters(){
    $('.section-filters').addClass('section-filters_active');
    $('.podstilka').show();
}
function hideFilters(){
    $('.section-filters').removeClass('section-filters_active');
    $('.podstilka').hide();
}
$('.section-head-filters').click(function(e){
    e.preventDefault();
    showFilters();
});
$('.section-filters__close').click(function(e){
    e.preventDefault();
    hideFilters();
});
$('.podstilka').click(function(e){
    e.preventDefault();
    hideFilters();
});

/**
 * CARD DESCRIPTION
 */
$('.section-card-description__title').click(function(e){
    e.preventDefault();
    const block = $(this).closest('.section-card-description');
    const text = block.find('.section-card-description__text');
    if (!block.hasClass('section-card-description_active')) {
        $(this).find('a').text('  -');
        block.addClass('section-card-description_active');
        text.slideDown(250);
    } else {
        $(this).find('a').text('  +');
        block.removeClass('section-card-description_active');
        text.slideUp(250);
    }
});

/**
 * CART EXPANDABLE
 *
 * @param triggerSelector
 * @param parentSelector
 * @param activeClass
 * @param innerSelector
 */
function cartExpandable(triggerSelector, parentSelector, activeClass, innerSelector) {
    $(triggerSelector).click(function(){
        const parent = $(this).closest(parentSelector);
        const inner = parent.find(innerSelector);
        const span = $(this).find('span');
        if (!parent.hasClass(activeClass)) {
            parent.addClass(activeClass);
            span.text('-');
            inner.slideDown(250);
        } else {
            parent.removeClass(activeClass);
            span.text('+');
            inner.slideUp(250);
        }
    })
}

cartExpandable('.section-cart__head', '.section-cart__section', 'section-cart__section_active', '.section-cart__blocks');

cartExpandable('.section-cart-block__title', '.section-cart__block', 'section-cart__block_active', '.section-cart-block__list');

// CARD ASIDE
$('.aside__hamburger').click(function(){
    if (!$('.aside_hidden').hasClass('aside_active')){
        $('.aside_hidden').addClass('aside_active')
        $('.aside__hamburger .hamburger').addClass('is-active')
        $('.aside_hidden .aside__nav').delay(250).fadeIn(250)
    } else {
        $('.aside__hamburger .hamburger').removeClass('is-active')
        $('.aside_hidden .aside__nav').fadeOut(250, function(){
            $('.aside_hidden').removeClass('aside_active')
        })

    }
})


if( $('.slider__slide').length ) {
    var height = $('.slider__slide.slick-slide.slick-current.slick-active').height();
    $('body .aside_index').css('height', height - 210);
    //console.log(height);
}
});


/**
* FILTERS COST RANGE
*
* @type {HTMLElement}
*/
const costSlider = document.getElementById('cost-range');

if (costSlider) {
var min=parseInt($(costSlider).attr("data-min"));
var max=parseInt($(costSlider).attr("data-max"));

noUiSlider.create(costSlider, {
    start: [min, max],
    range: {
        'min': [min],
        'max': [max]
    },
    step: 10,
    format: {
        // 'to' the formatted value. Receives a number.
        to: function (value) {
            return Math.round(value);
        },
        // 'from' the formatted value.
        // Receives a string, should return a number.
        from: function (value) {
            return Math.round(value);
        }
    }
});

var costValues = [
    document.getElementById('cost-from'),
    document.getElementById('cost-to')
];

costSlider.noUiSlider.on('update', function (values, handle) {
    costValues[handle].innerHTML = values[handle];
    // sendAjaxSmartFilterForm();
});

costSlider.noUiSlider.on('change', function (values, handle) {
    // costValues[handle].innerHTML = values[handle];
    sendAjaxSmartFilterForm();
});
}

/* ugrm@uvelirsoft.ru ---07/10/22--- */

function goBackPage(sectionUrl){
    if(document.referrer.indexOf(window.location.host) >= 0){
        window.history.back();
    }else if(sectionUrl.length > 0){
        window.location = sectionUrl;
    }
}

function parseParamsInUrlSearch(){
    var query = location.search.replace('?', ''),
        arQuery = query.split('&');

    return arQuery; 
}

function setParamInUrlSearch(newParam, delParamName){
    if(typeof newParam == 'undefined') return '';
    
    var arQuery = parseParamsInUrlSearch(), 
        _arQuery = [],
        queryStr;

    if(typeof delParamName != 'string') delParamName = '';

    for(var i = 0; i < arQuery.length; i++){
        if(
            arQuery[i] != '' 
            && (
                delParamName == ''
                || (delParamName != '' && arQuery[i].indexOf(delParamName) < 0)
            )
            && arQuery[i].indexOf('productId=') < 0
        ){
            _arQuery.push(arQuery[i]);
        }
    }
    
     _arQuery.push(newParam);
     
    return '?' + _arQuery.join('&');
}

$(document).on('click', '.section-catalog-card a', function(){
    // var productId = $(this).parents('.section-catalog-card').find('a[name]').attr('name'),
    var productId = $(this).parents('.section-catalog-card').attr('data-item-id'),
        queryStr;
        
    queryStr = setParamInUrlSearch('productId='+productId, '');
    //history.pushState({}, '', queryStr); 
    history.replaceState({}, '', queryStr); 
});

$(document).ready(function(){
    var arQuery = parseParamsInUrlSearch(),
        productId = '';
    
    for(var i = 0; i < arQuery.length; i++){
        if(arQuery[i] != '' && arQuery[i].indexOf('productId=') >= 0){
            productId = arQuery[i].replace('productId=', '');
        }
    }
    
    if(productId != ''){
        // var obItem = $('.section-catalog-card a[name="'+productId+'"]');       
        // obItem = obItem.length ? obItem : $('[name="'+productId+'"]');

        var obItem = $('.section-catalog-card a[data-item-id="'+productId+'"]');       
        obItem = obItem.length ? obItem : $('[data-item-id="'+productId+'"]');

        if(obItem.length)
            return $(".os-viewport").animate({scrollTop: obItem.eq(0).offset().top}, 200), !1;
            //return $("html,body").animate({scrollTop: obItem.eq(0).offset().top}, 200), !1;
    }
});

$(document).on('change', '#price_sort_select', function(){
    window.location = '?sort=' + this.value;
});

;
; /* Start:"a:4:{s:4:"full";s:53:"/local/templates/adamant/js/custom.js?163879836911194";s:6:"source";s:37:"/local/templates/adamant/js/custom.js";s:3:"min";s:0:"";s:3:"map";s:0:"";}"*/
$(function() {

    var moreNews = function moreNews(event) {
        if ($("#ajax_next_page").length) {
            $(".show_more_news").hide();
            var url = $("#ajax_next_page").attr("href");
            $.ajax({
                url: url,
                type: "POST",
                data: {IS_AJAX: 'Y'},
                success: function (data) {
                    $("#ajax_next_page").after(data);
                    $("#ajax_next_page").remove();
                }
            });
        }
    }

    var showMore = function showMore(event) {

        $(".show_more").remove();
        if ($("#ajax_next_page").length) {
            var url = $("#ajax_next_page").attr("href");
            $(".section__bottom").hide();
            $.ajax({
                url: url,
                type: "POST",
                data: {IS_AJAX: 'Y'},
                success: function (data) {
                    if ($('#orders_history').length) {
                        //$(".section__bottom").remove();
                        $('#orders_history').append($(data).find("tr"));                       
                        $("#ajax_wrap").append($(data).filter(".section__bottom"));
                    }
                    else if ($('#section-promotions__cards').length) {
                        $('#section-promotions__cards').append($(data).find(".section-promotions__card"));                       
                        $("#ajax_wrap").append($(data).filter(".section__bottom"));
                    }
                    else{
                        $("#ajax_wrap").append(data);
                    }
                    
                    $("#ajax_next_page").remove();

                    history.pushState(null, null, url)
                }
            });
        }
    }

    var sendRegistrationForm = function sendRegistrationForm(event){
        event.preventDefault();
        var $this = $(event.currentTarget);
        var data=$this.serialize();
        var url=$this.data("ajaxurl");
        $.ajax({
            url: url,
            method: $(this).attr("method"),
            data:new FormData(this),
            contentType: false,
            cache: false,
            processData: false,
            success: function(data){
                $('.register-ajax-wrapper').empty();
                $('.register-ajax-wrapper').append(data);
                $("input[name='PHONE']").mask('+7 000 000 0000', {clearIfNotMatch: false});

            },
            error: function(data){
                $('.register-ajax-wrapper').empty();
                $('.register-ajax-wrapper').append(data);
            }
        });



    }

    var sendPersonalForm = function sendPersonalForm(event){
        event.preventDefault();
        var $this = $(event.currentTarget);
        var data=$this.serialize();
        var url=$this.data("ajaxurl");

        $.ajax({
            url: url,
            method: $(this).attr("method"),
            data:new FormData(this),
            contentType: false,
            cache: false,
            processData: false,
            success: function(data){
                $('.personal-ajax-wrapper').empty();
                $('.personal-ajax-wrapper').append(data);

            },
            error: function(data){
                $('.personal-ajax-wrapper').empty();
                $('.personal-ajax-wrapper').append(data);
            }
        });

    }




    $(document).on("click",".show_more_news",moreNews);
    $(document).on("click",".show_more",showMore);
    $(document).on("submit",".registration",sendRegistrationForm);
    $(document).on("submit",".personal",sendPersonalForm);

});

$(document).ready(function() {
    /**
     *         
     */
    getBasket();
});

/**
 *  
 */
function getBasket() {
    /**
     *   
     */
    $.ajax({
        url: '/local/ajax/getBasket.php',
        dataType: 'json',
        method: 'post',
        success: function(data){
            if (data.success == 1 && data.error == '') {
                loadBasketDataToHeader(data);
                loadBasketDataToElementCard(data);
            }
        },
        complete: function(data) {
        }
    });
}

/**
 *      
 */
function loadBasketDataToHeader(data) {
    /**
     *        
     */
    if (data.sum > 0) {
        $("#header_cart_is_empty").each(function() {
            $(this).css("display", "none");
        });
        $("#header_cart_is_full").each(function() {
            $(this).css("display", "block");
        });
    }

    /**
     *   
     */
    $(".header__cart #count_elements").each(function() {
        $(this).html(data.count_offers);
    });
    $(".header__cart #count_vendors").each(function() {
        $(this).html(data.count_vendors);
    });
    $(".header__cart #cart_weight").each(function() {
        $(this).html(data.weight);
    });
    $("#header_cart_sum").each(function() {
        $(this).html(data.sum_formatted);
    });
}

/**
 *       
 */
function loadBasketDataToElementCard(data)
{

    /**
     *     
     */
    $.each(data.products_quantity, function(key, item) {
        $('#warehouse_main_element_counter_' + item.product_id).each(function() {
            $(this).html(item.quantity_warehouse);
        });
    });
    $.each(data.products_quantity, function(key, item) {
        $('#order_main_element_counter_' + item.product_id).each(function() {
            $(this).html(item.quantity_order);
        });
    });
    $.each(data.products_quantity, function(key, item) {
        $('#all_main_element_counter_' + item.product_id).each(function() {
            $(this).html(item.quantity);
        });
    });

    //        data
    $('.main_element_counter').each(function() {
        let id = $(this).data("element-id");
        if(typeof data.products[id] === "undefined"){
            $('#warehouse_main_element_counter_' + id).each(function() {
                $(this).html(0);
            });
            $('#order_main_element_counter_' + id).each(function() {
                $(this).html(0);
            });
            $('#all_main_element_counter_' + id).each(function() {
                $(this).html(0);
            });
        }
    });


    /**
     *    
     */
    $.each(data.offers, function(key, item) {
        $('#quantity_warehouse_'+ item.id +'_' + item.product_id).each(function() {
            $(this).val(item.quantity);
            if(item.quantity>0){
                $(this).parent().addClass("notempty");
                $(this).parent().parent(".section-card-tools__col").addClass("section-card-tools__counter_active");
            }

        });
    });
}

/**
 *      -  
 *
 * @param url
 */
function updateBanners(url) {

  //  return;

    /**
     *   
     */
    $.ajax({
        url: '/local/ajax/getBannersForSection.php?sub_url=' + url,
        method: 'get',
        success: function(data){
            //   
            let res1 = "";
            $(data).each(function(){
                if(this.nodeName == "IMG"){
                    res1 = res1 + $(this).attr('src');
                }
            })
            let res2 = "";
            $('#slider_ajax_wrap').children("img").each(function(){
                if(this.nodeName == "IMG"){
                    res2 = res2 + $(this).attr('src');
                }
            })

            if(res1 !== res2) {
                $('#slider_ajax_wrap').html(data);
            }
            history.pushState(null, null, url);
        },
        complete: function(data) {
        }
    });
}

/**
 *      get 
 *
 * @param key
 *
 * @returns {*}
 */
function $_GET(key) {
    var p = window.location.search;
    p = p.match(new RegExp(key + '=([^&=]+)'));
    return p ? p[1] : false;
}

/**
 *    PHP
 *
 * @param mixed_var
 *
 * @returns {boolean|*}
 */
function empty(mixed_var) {
    return ( mixed_var === "" || mixed_var === 0   || mixed_var === "0" || mixed_var === null  || mixed_var === false
        ||  ( is_array(mixed_var) && mixed_var.length === 0 ) );
}

/**
 *    PHP
 *
 * @param mixed_var
 *
 * @returns {boolean}
 */
function is_array( mixed_var ) {
    return ( mixed_var instanceof Array );
}

/**
 *      
 */
function sendAjaxSmartFilterForm() {

    var url = $('#adamant_custom_smart_filter_form').attr('action');

    /**
     *      
     */
    var data_val = $('#cost-from').html() + '-' + $('#cost-to').html();

    if (/[?&]priceFilter\s*=/.test(url)) {
        url = url.replace(/(?:([?&])priceFilter\s*=[^?&]*)/, "priceFilter=" + data_val);
    } else if (/\?/.test(url)) {
        url = url + "&priceFilter=" + data_val;
    } else {
        url = url + "?priceFilter=" + data_val;
    }

    /**
     *    
     */
    var selectedSort = $("#price_sort_select").val();

    if (empty(selectedSort)) {
        selectedSort = 'ASC';
    }

    if (/[?&]priceSort\s*=/.test(url)) {
        url = url.replace(/(?:([?&])priceSort\s*=[^?&]*)/, "priceSort=" + selectedSort);
    } else if (/\?/.test(url)) {
        url = url + "&priceSort=" + selectedSort;
    } else {
        url = url + "?priceSort=" + selectedSort;
    }

    /**
     *    
     */
    if (/[?&]IS_AJAX\s*=/.test(url)) {
        url = url.replace(/(?:([?&])IS_AJAX\s*=[^?&]*)/, "IS_AJAX=Y");
    } else if (/\?/.test(url)) {
        url = url + "&IS_AJAX=Y";
    } else {
        url = url + "?IS_AJAX=Y";
    }

    /**
     *  
     */
    $.ajax({
        url: url,
        method: 'get',
        dataType: 'html',
        data: $('#adamant_custom_smart_filter_form').serialize(),
        success: function(data){
            $('#ajax_wrap').html(data);
        }
    });
}

/**
 *  /   
 *
 * @param element
 */
function toggleHiddenDiv(element) {
    element.toggle();
}

$(document).on("click",".scroll_link",function(e){
  
    $(this).closest("ul").find("a").removeClass("active");
    $(this).addClass("active");    
})



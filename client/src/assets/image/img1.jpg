                        // return x;
                        return x.toString().replace(/\B(?=(\d{3})+(?!\d))/g, " ");
                    }

                    function setCreditPrices()
                    {
                        var specialPrice = $('.product-info-main .price').html();
                        var specialPrice = specialPrice.match(/\d/g);

                        var mPrice24 = withCard / monthly24;
                        mPrice24 = Math.ceil(mPrice24 / 10) * 10;
                        var mPrice36 = withCard / monthly36;
                        mPrice36 = Math.ceil(mPrice36 / 10) * 10;

                        $('.with_card_price').html(currencySymbol + numberWithCommas(withCard))
                        $('.with_cash_price').html(currencySymbol + numberWithCommas(specialPrice))
                        $('.mPrice_12').html(currencySymbol + numberWithCommas(mPrice12))
                        $('.mPrice_24').html(currencySymbol + numberWithCommas(mPrice24))
                        $('.mPrice_36').html(currencySymbol + numberWithCommas(mPrice36))
                    }
